# mute volume
cl_setvar audioenabled 1
# show debug info in console
cl_setvar debug 0
# save console log to file on close
cl_setvar debuglog 0
# 0=window||1=borderless||2=fullscreen
cl_setvar displaymode 0
# font name ui
cl_setvar fontui None
# available framerates
cl_setvar framerates 24,30,60,75,98,120,144,165,240,320,360
# default net address
cl_setvar joinip localhost
# default net port
cl_setvar joinport 5555
# player color
cl_setvar playercolor blue
# player name
cl_setvar playername player
# available resolutions
cl_setvar resolutions 640x480,800x600,1024x768,1280x720,1280x1024,1680x1050,1600x1200,1920x1080,2560x1440,3840x2160
# show frames per second
cl_setvar showfps 1
cl_setvar showcam 1
cl_setvar showmouse 1
cl_setvar shownet 1
cl_setvar showplayer 1
cl_setvar showtick 1
cl_setvar showscale 1
# enable animations
cl_setvar vfxenableanimations 1
# enable flares
cl_setvar vfxenableflares 1
# enable shading
cl_setvar vfxenableshading 1
# enable shadows
cl_setvar vfxenableshadows 1
# video mode
cl_setvar vidmode 1280,720,60
# volume
cl_setvar volume 50
# bindings
bind - cl_exec scripts/zoomout
bind = cl_exec scripts/zoomin
bind backspace e_delthing
bind w exec scripts/playerup
bind s exec scripts/playerdown
bind a exec scripts/playerleft
bind d exec scripts/playerright
bind tab exec scripts/showscore
bind enter activatemenu
bind escape gobackui
bind up exec scripts/selectup
bind down exec scripts/selectdown
bind left exec scripts/selectleft
bind right exec scripts/selectright
bind y chat
-bind ` console
-bind w exec scripts/playerupx
-bind s exec scripts/playerdownx
-bind a exec scripts/playerleftx
-bind d exec scripts/playerrightx
-bind tab exec scripts/showscorex
-bind up exec scripts/playerupx
-bind down exec scripts/playerdownx
-bind left exec scripts/playerleftx
-bind right exec scripts/playerrightx
-bind r e_rotthing
