# port clients connect to
setvar listenport 5555
# max hp
setvar maxhp 500
# timelimit in ms
setvar timelimit 120000
# respawn wait time in ms
setvar respawnwaittime 3000
# player speed
setvar velocityplayerbase 64
# number of votes to skip map (say 'skip' in chat)
setvar voteskiplimit 2
# time between game end and next map loads
setvar intermissiondelay 10000
